(function($, window) {
  $(function() {
    // ここに処理を書きます
    $(widow).ready( function() {
	$('#btn_postcode2address').jpostal({
		click : '#btn_postcode2address',
		postcode : [
			'#postcode'
		],
		address : {
			'#pref'  : '%3',
			'#city'  : '%4',
			'#street'  : '%5'
		}
	});
});
});
})(jQuery, window);
